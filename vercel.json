{
    "builds": [
        {
            "src": "/api/main.py",
            "use": "@vercel/python"
        },
        {
            "src": "/frontend/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "/frontend/dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/main.py"
        },
        {
            "src": "/(.*)",
            "dest": "/frontend/dist/$1"
        }
    ]
}